<div class="mybody">
<div class="sign-up-page">
<div class="sign-up-field"> 
<h3 class="sign-up-text">Sign up to continue</h3>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div class="form-group form-group1">
    <%# <%= f.label :first_name %>
    <%= f.text_field :first_name, autofocus: true, class: "form-control form-control1", placeholder: "First Name" %>
  
    <%#<%= f.label :last_name %>
    <%= f.text_field :last_name, class: "form-control form-control1", placeholder: "Last Name" %>
  
    <%#<%= f.label :email %>
    <%= f.email_field :email, class: "form-control form-control1", placeholder: "Email address" %>
  
    <%#<%= f.label :password %>
    <% if @minimum_password_length %>
    <%#<em>(<%= @minimum_password_length %> <%#characters minimum)</em>%>
    <% end %>
    <%= f.password_field :password, autocomplete: "off", class: "form-control form-control1", placeholder: "Password" %>
  
    <%#<%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control form-control1", placeholder: "Confirm Password" %>
  
    <%= f.submit "Sign up", class: "btn btn-primary" %><br>
    <span class="have-account"> Already have an account? </span> <%= render "devise/shared/links" %>
    <%= link_to "Sign up with Facebook", user_facebook_omniauth_authorize_path %> 

    
  </div>
<% end %>

</div>
</div>
</div>


